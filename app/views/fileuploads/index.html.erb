<p id="notice"><%= notice %></p>

<h1>Fileuploads</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Filepath</th>
      <th>Thumbnail img path</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fileuploads.each do |fileupload| %>
      <tr>
        <td><%= fileupload.name %></td>
        <td><%= fileupload.filepath %></td>
        <td><%= fileupload.thumbnail_img_path %></td>
        <td><%= link_to 'Show', fileupload %></td>
        <td><%= link_to 'Edit', edit_fileupload_path(fileupload) %></td>
        <td><%= link_to 'Destroy', fileupload, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fileupload', new_fileupload_path %>
